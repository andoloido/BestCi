<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="10000">
        <KeyFrameSet>
            <KeyPosition
                motion:framePosition="13"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/contentCl1"
                motion:percentX="0.25"
                motion:percentY="0.65" />
            <KeyPosition
                motion:framePosition="26"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/contentCl1"
                motion:percentX="0"
                motion:percentY="0.5" />
            <KeyPosition
                motion:framePosition="39"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/contentCl1"
                motion:percentX="0.5"
                motion:percentY="0.25" />
            <KeyPosition
                motion:framePosition="52"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/contentCl1"
                motion:percentX="1"
                motion:percentY="0.5" />
            <KeyPosition
                motion:framePosition="65"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/contentCl1"
                motion:percentX="0.5"
                motion:percentY="0.75" />
            <KeyPosition
                motion:framePosition="78"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/contentCl1"
                motion:percentX="0.25"
                motion:percentY="0.65" />
            <KeyPosition
                motion:framePosition="91"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/contentCl1"
                motion:percentX="0.5"
                motion:percentY="0.5" />

            <KeyPosition
                motion:framePosition="12"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/contentCl2"
                motion:percentX="0.25"
                motion:percentY="0.65" />
            <KeyPosition
                motion:framePosition="25"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/contentCl2"
                motion:percentX="0"
                motion:percentY="0.5" />
            <KeyPosition
                motion:framePosition="38"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/contentCl2"
                motion:percentX="0.5"
                motion:percentY="0.25" />
            <KeyPosition
                motion:framePosition="51"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/contentCl2"
                motion:percentX="1"
                motion:percentY="0.5" />
            <KeyPosition
                motion:framePosition="64"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/contentCl2"
                motion:percentX="0.5"
                motion:percentY="0.75" />
            <KeyPosition
                motion:framePosition="77"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/contentCl2"
                motion:percentX="0.25"
                motion:percentY="0.65" />
            <KeyPosition
                motion:framePosition="90"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/contentCl2"
                motion:percentX="0.5"
                motion:percentY="0.5" />
        </KeyFrameSet>
        <OnClick motion:targetId="@+id/clickBt" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/contentCl1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/contentCl2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/contentCl1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toStartOf="@id/contentCl2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />

        <Constraint
            android:id="@+id/contentCl2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@id/contentCl1"
            motion:layout_constraintTop_toTopOf="parent" />
    </ConstraintSet>
</MotionScene>